    "Finding Triangle within SQL--\n",
    "SELECT \n",
    "    CASE\n",
    "        WHEN a + b > c AND a + c > b AND b + c > a THEN 'Yes' -- If the sum of any two sides is greater than the third side, it forms a triangle\n",
    "        ELSE 'No'\n",
    "    END AS is_triangle\n",
    "FROM \n",
    "    triangle;"
